{
  "name": "videojs-google-chromecast",
  "version": "1.0.2",
  "description": "Chromecast plugin for videojs, adapted for v7.*",
  "main": "main/js/plugin.js",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "npm-run-all -p build:*",
    "build:js": "npm-run-all mkdirs build:js:babel build:js:browserify build:js:bannerize build:js:uglify copy",
    "build:js:babel": "npx babel src -d main",
    "build:js:bannerize": "bannerize dist/videojs-google-chromecast.js --banner=scripts/banner.ejs",
    "build:js:browserify": "browserify . -s videojs-google-chromecast -o dist/videojs-google-chromecast.js",
    "build:js:uglify": "uglifyjs dist/videojs-google-chromecast.js --comments --mangle --compress -o dist/videojs-google-chromecast.min.js",
    "build:css": "node-sass --include-path src --output-style compressed src/css/videojs-google-chromecast.scss -o dist",
    "build:test": "npm-run-all mkdirs build:test:browserify",
    "build:test:browserify": "browserify `find test -name '*.test.js'` -t babelify -o dist-test/videojs-google-chromecast.js",
    "copy:font": "cp -R src/fonts dist/fonts",
    "copy": "npm-run-all -p copy:*",
    "clean": "rm -rf dist dist-test main",
    "docs": "doctoc README.md docs/api.md",
    "lint": "vjsstandard",
    "mkdirs": "mkdir -p dist dist-test main",
    "prepare": "npm run build",
    "prestart": "npm-run-all -p docs build",
    "dev": "echo 'requiring node v4.5.0' && . ~/.nvm/nvm.sh && nvm use 4.5.0 && npm-run-all -p start watch",
    "pretest": "npm-run-all build:test",
    "test": "karma start test/karma/detected.js",
    "test:chrome": "npm run pretest && karma start test/karma/chrome.js",
    "test:firefox": "npm run pretest && karma start test/karma/firefox.js",
    "test:ie": "npm run pretest && karma start test/karma/ie.js",
    "test:safari": "npm run pretest && karma start test/karma/safari.js",
    "watch": "npm run mkdirs && npm-run-all -p watch:*",
    "watch:js": "watchify src/js/plugin.js -t babelify -v -o dist/videojs-google-chromecast.js",
    "watch:css": "nodemon -e scss -x \"npm run build:css\"",
    "watch:test": "watchify `find test -name '*.test.js'` -t babelify -o dist-test/videojs-google-chromecast.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikadoplus/videojs-google-chromecast.git"
  },
  "keywords": [
    "videojs",
    "chromecast",
    "video.js",
    "videojs-plugin"
  ],
  "author": "Patrick LJ",
  "license": "Apache 2.0",
  "dependencies": {
    "object.assign": "^4.1.0",
    "video.js": "^7.7.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-react-transform": "^3.0.0",
    "babelify": "^10.0.0",
    "bannerize": "^1.1.4",
    "browserify": "^16.5.1",
    "browserify-shim": "^3.8.14",
    "doctoc": "^1.4.0",
    "global": "^4.4.0",
    "karma": "^5.0.5",
    "karma-browserify": "^7.0.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-detect-browsers": "^2.3.3",
    "karma-firefox-launcher": "^1.3.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-qunit": "^4.1.1",
    "karma-safari-launcher": "^1.0.0",
    "karma-sinon-ie": "^2.0.0",
    "lodash-compat": "^3.10.2",
    "minimist": "^1.2.5",
    "node-sass": "^4.14.1",
    "node.extend": "^2.0.2",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "qunit": "^2.10.0",
    "sinon": "^9.0.2",
    "uglify-js": "^3.9.2",
    "videojs-standard": "^8.0.4",
    "watchify": "^3.11.1",
    "webpack": "^4.43.0",
    "xhr": "^2.5.0"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      [
        "babelify",
        {
          "presets": [
            "@babel/preset-react",
            "@babel/preset-env"
          ]
        }
      ]
    ]
  },
  "browserify-shim": {
    "qunit": "global:QUnit",
    "sinon": "global:sinon",
    "video.js": "global:videojs"
  },
  "vjsstandard": {
    "ignore": [
      "dist",
      "dist-test",
      "docs",
      "main",
      "test/karma",
      "scripts"
    ]
  },
  "bugs": {
    "url": "https://github.com/mikadoplus/videojs-google-chromecast/issues"
  },
  "homepage": "https://github.com/mikadoplus/videojs-google-chromecast#readme"
}
